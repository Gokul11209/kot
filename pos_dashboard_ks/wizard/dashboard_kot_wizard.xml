<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="dashboard_kot_wizard" model="ir.ui.view">
        <field name="name">kot.screen.wizard</field>
        <field name="model">kot.screen.wizard</field>
        <field name="arch" type="xml">
            <form>
                <footer>
                    <button name="tick_ok" type='object' string="Submit" class="oe_highlight"
                            icon="fa-check-circle"/>
                    <button name="tick_cancel" type='object' string="Cancel" class="oe_highlight" special="cancel"
                            icon="fa-times-circle"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
